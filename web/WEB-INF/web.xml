<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MatrixInfoController</servlet-name>
        <servlet-class>advprogclasses.MatrixInfoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResultController</servlet-name>
        <servlet-class>advprogclasses.ResultController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>advprogclasses.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionLogInfoController</servlet-name>
        <servlet-class>advprogclasses.SessionLogInfoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionLogDeleteController</servlet-name>
        <servlet-class>advprogclasses.SessionLogDeleteController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MatrixInfoController</servlet-name>
        <url-pattern>/MatrixInfoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResultController</servlet-name>
        <url-pattern>/ResultController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionLogInfoController</servlet-name>
        <url-pattern>/SessionLogInfoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionLogDeleteController</servlet-name>
        <url-pattern>/SessionLogDeleteController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
